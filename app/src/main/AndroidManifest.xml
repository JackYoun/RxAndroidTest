<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jack.app.rxtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".fundamental.FundamentalActivity"
            android:label="@string/fundamental"/>
        <activity
            android:name=".operator.OperatorActivity"
            android:label="@string/operator"/>
        <activity
            android:name=".scheduler.SchedulerActivity"
            android:label="@string/scheduler"/>
        <activity
            android:name=".rxbinding.RxBindingActivity"
            android:label="@string/rxbinding"/>
    </application>

</manifest>